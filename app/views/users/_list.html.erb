<!-- 以下部分テンプレ -->
<h1>User info</h1>
  <table class="table">
    <tbody>
      <!--ここに画像挿入-->
      <tr>
      <!--ログインしている人の名前-->
      <th>name</th>
      </tr>
      <tr>
        <!-- ログインしている人の紹介文-->
      <th>introduction</th>
      </tr>
    </tbody>
  </table>
  <%= link_to "プロフを編集", edit_user_path(users),  class: "btn btn-secondary col-xs-12" %>

<h2>New book</h2>
<%= form_for(@book) do |f| %>
  <label>Title</label>
  <br>
  <%= f.text_field :title, class: "col-xs-12 book_title" %>
  <br>
  <label>opinion</label>
  <br>
  <%= f.text_area :body, class: "col-xs-12 book_body" %>
  <br><br><br>
  <div class="actions">
  <%= f.submit 'Create Book', class: "btn btn-primary col-xs-12" %>
  </div>
<% end %>